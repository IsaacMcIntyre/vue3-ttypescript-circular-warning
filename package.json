    "@vue/tsconfig": "^0.1.3",
    "@vue/vue3-jest": "^29.2.2",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jest-environment-jsdom": "^29.4.2",
    "jest-junit": "^15.0.0",
